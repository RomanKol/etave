{
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "globals": {
    "chrome": true,
    "React": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "jsx-a11y/heading-has-content": "disable",
    "react/jsx-indent": "disabled",
    "react/sort-comp": "disable",
    "react/destructuring-assignment": "never"
  }
}
