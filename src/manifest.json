{
    "manifest_version": 2,
    "name": "etave 2",
    "description": "event tracking and visualization extension",
    "version": "0.0.5",
    "minimum_chrome_version": "58",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup/popup.html",
        "default_title": "Etave 2 session"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "options_ui": {
        "page": "options/options.html",
        "chrome_style": false,
        "open_in_tab": true
    },
    "icons": {
        "128": "icon.png"
    },
    "permissions": [
        "activeTab",
        "downloads",
        "storage",
        "tabs",
        "<all_urls>"
    ]
}
